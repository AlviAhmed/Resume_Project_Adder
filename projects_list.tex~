\renewcommand{\maketitle}{
  \begin{center}
    \phantom{.} %if I did not add this, vspace would not work for some reason
    % phantom is used to create space, in this case a blank space
    \vspace{-2em}
    {\Huge\bfseries \thetitle}
    \vspace{-0.5em}
    \par\noindent\rule{\textwidth}{2pt}
  \end{center} 
  \begin{center} 
    \vspace{-1em}  
    {\small\theauthor}
  \end{center}
} 

